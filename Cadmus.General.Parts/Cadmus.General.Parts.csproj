<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
	  <Nullable>enable</Nullable>
	  <IncludeSymbols>true</IncludeSymbols>
	  <SymbolPackageFormat>snupkg</SymbolPackageFormat>
	  <Version>8.0.0</Version>
	  <Authors>Daniele Fusi</Authors>
	  <Company>Fusi</Company>
	  <Product>Cadmus</Product>
	  <Description>Generic parts for Cadmus.</Description>
	  <Copyright>by Daniele Fusi 2022</Copyright>
	  <NeutralLanguage>en-US</NeutralLanguage>
	  <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
	  <PackageTags>Cadmus;parts</PackageTags>
	  <FileVersion>8.0.0</FileVersion>
	  <AssemblyVersion>8.0.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Cadmus.Core" Version="10.0.0" />
    <PackageReference Include="Cadmus.Mat.Bricks" Version="7.0.0" />
    <PackageReference Include="Cadmus.Refs.Bricks" Version="7.0.0" />
  </ItemGroup>

</Project>
